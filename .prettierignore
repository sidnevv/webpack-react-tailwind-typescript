# Ignore artifacts:
.idea
dist
build
coverage
node_modules

# Ignore all HTML files:
*.html

# Ignore config files:
webpack.config.js
tailwind.config.js
postcss.config.js
tsconfig.json

# Ignore package files:
package-lock.json
yarn.lock

# Ignore logs:
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*